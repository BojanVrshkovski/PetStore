type Query {
    users: [User]
}



type User {
    userId: ID!
    firstName: String!
    lastName: String!
    email: String!
    budget: Float!
}

type Pet {
    petId: ID!
    owner: User!
    name: String!
    petType: String!
    description: String
    dateOfBirth: String!
    price: Float!
    rating: Int
}